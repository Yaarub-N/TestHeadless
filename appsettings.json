{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
        "Global": {
            "Id": "9071da31-b4d8-4e6d-a0eb-1a45573ff86c",
            "SanitizeTinyMce": true
        },
        "Content": {
            "AllowEditInvariantFromNonDefault": true,
            "ContentVersionCleanupPolicy": {
                "EnableCleanup": true
            }
        },
        "Unattended": {
            "UpgradeUnattended": true
        },
        "Security": {
            "AllowConcurrentLogins": false
        },
        "DeliveryApi": {
            "Enabled": true
        },
        "ModelsBuilder": {"ModelsMode": "SourceCodeAuto"}


    }

  },
    "ConnectionStrings": {
        //  "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
        "umbracoDbDSN": "Server=tcp:umbracotest.database.windows.net,1433;Initial Catalog=TestUmbracoDatabase;Persist Security Info=False;User ID=admin1;Password=Test.123",
        "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
    }
}